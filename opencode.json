{
  "model": "ollama/qwen3-coder:30b-a3b-q4_K_M",
  "instructions": [
    "CONTRIBUTING.md",
    "docs/guidelines.md",
    ".cursor/rules/*.md"
  ],
  "theme": "transparent",
  "provider": {
    "ollama": {
      "npm": "@ai-sdk/openai-compatible",
      "name": "Ollama",
      "options": {
        "baseURL": "http://127.0.0.1:11434/v1"
      },
      "models": {
        "dolphin3:latest": {
          "name": "dolphin3:latest",
          "reasoning": true
        },
        "deepseek-r1:1.5b": {
          "name": "deepseek-r1:1.5b",
          "reasoning": true,
          "tool_call": false
        },
        "qwen2.5-coder:14b": {
          "name": "qwen2.5-coder:14b"
        },
        "gemini-3-flash-preview:latest": {
          "name": "gemini-3-flash-preview:latest"
        },
        "llama3.2:latest": {
          "name": "llama3.2:latest"
        },
        "qwen3-coder:30b-a3b-q4_K_M": {
          "name": "qwen3-coder:30b-a3b-q4_K_M"
        }
      }
    },
    "google": {
      "models": {
        "gemini-3-pro-high": {
          "name": "Gemini 3 Pro High",
          "id": "gemini-3-pro-preview",
          "options": {
            "thinkingConfig": {
              "thinkingLevel": "high",
              "includeThoughts": true
            }
          }
        },
        "gemini-3-pro-low": {
          "name": "Gemini 3 Pro Low",
          "id": "gemini-3-pro-preview",
          "options": {
            "thinkingConfig": {
              "thinkingLevel": "low"
            }
          }
        },
        "gemini-3-flash": {
          "name": "Gemini 3 Flash",
          "attachment": true,
          "limit": {
            "context": 1048576,
            "output": 65536
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          }
        }
      }
    }
  },
  "plugin": [
    "opencode-gemini-auth@latest",
    "oh-my-opencode"
  ],
  "$schema": "https://opencode.ai/config.json"
}
